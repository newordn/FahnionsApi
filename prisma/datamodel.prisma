type Tshirt{
    id: ID! @id
    title: String!
    description: String!
    price: String!
    createdAt: DateTime! @createdAt
    images: [String!]! @scalarList(strategy: RELATION)
    contact: String!
}
type Pub{
    id:ID! @id
    enterprise:String!
    enterpriseDescription:String!
    title: String!
    description:String!
    createdAt: DateTime! @createdAt
    medias: [String!]! @scalarList(strategy: RELATION)
    contact: String
    status : Boolean!
}
type Profil{
    id: ID! @id
    name: String!
    description: String!
    pays: String!
    ville: String!
    quartier: String!
    email: String
    phone: String!
    password: String!
    avatar: String!
    createdAt: DateTime! @createdAt
}
type Shop{
    id: ID! @id
    name: String!
    description: String!
    pays: String!
    ville: String!
    localisation: String!
    contact: String!
    affiche: String!
    password: String!
    articles: [Article] @relation(name:"ShopArticle")
    createdAt: DateTime! @createdAt
}
type Article {
    id: ID! @id
    name: String!
    description: String!
    price: String!
    shop: Shop! @relation(name:"ShopArticle")
    images: [String!]! @scalarList(strategy: RELATION)
    createdAt: DateTime! @createdAt
}