type Tshirt{
    id: ID! @id
    title: String!
    description: String!
    price: String!
    createdAt: DateTime! @createdAt
    images: [String!]! @scalarList(strategy: RELATION)
    contact: String!
}
type Pub{
    id:ID! @id
    title: String!
    description:String!
    createdAt: DateTime! @createdAt
    medias: [String!]! @scalarList(strategy: RELATION)
    contact: String
}