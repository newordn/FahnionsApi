type Tshirt{
    id: ID! @id
    title: String!
    description: String!
    price: String!
    createdAt: DateTime! @createdAt
    images: [String!]! @scalarList(strategy: RELATION)
    contact: String!
}
type Pub{
    id:ID! @id
    enterprise:String!
    enterpriseDescription:String!
    title: String!
    description:String!
    createdAt: DateTime! @createdAt
    medias: [String!]! @scalarList(strategy: RELATION)
    contact: String
    status : Boolean!
}
type Profil{
    id: ID! @id
    name: String!
    pays: String!
    ville: String!
    quartier: String!
    email: String
    phone: String!
    password: String!
    avatar: String!
    createdAt: DateTime! @createdAt
}